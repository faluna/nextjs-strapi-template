{
  "name": "nextjs-strapi-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "build": "aspida && next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "export": "rm -f ./public/sitemap.xml && next build && node src/utils/generate-sitemap.js && next export",
    "serve": "yarn export && serve ./out",
    "generate-sitemap": "node src/utils/generate-sitemap.js",
    "test": "jest",
    "prepare": "husky install",
    "dev:aspida": "aspida --watch",
    "build:api": "aspida",
    "dev": "yarn dev:aspida && yarn dev:next"
  },
  "dependencies": {
    "@aspida/axios": "^1.6.3",
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.3.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "framer-motion": "^3.6.2",
    "next": "10.0.7",
    "next-seo": "^4.20.0",
    "nextjs-sitemap-generator": "^1.3.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-aspida": "^0.0.2",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
